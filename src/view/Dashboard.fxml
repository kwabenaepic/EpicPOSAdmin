                                          </HBox>
                                       </children>
                                    </HBox>
                                                <Label fx:id="lblUserId" prefHeight="17.0" prefWidth="58.0" styleClass="labelColor" text="COLOR" visible="false" />
                                            </children>
                                        </HBox>
                                        <Region HBox.hgrow="ALWAYS" />
