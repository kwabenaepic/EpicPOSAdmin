                                          <ImageView fitHeight="27.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true">
                                             <image>
                                                <Image url="@../images/lock.png" />
                                             </image>
                                          </ImageView>
                                       </graphic>
                                    </Button>
                                            </children>
                                 <padding>
                                    <Insets right="15.0" />
                                 </padding>
                                        </HBox>
                                    </children>
                                </HBox>
                            </children>
                        </AnchorPane>
                    </top>
                </BorderPane>
            </children>
        </StackPane>
    </children>
    <stylesheets>
        <URL value="@../css/Dashboard.css" />
        <URL value="@../css/custom.css" />
    </stylesheets>
</AnchorPane>
